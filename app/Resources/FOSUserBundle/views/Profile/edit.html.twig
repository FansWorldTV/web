{% set user = app.user %}
{% extends "DodiciFansworldWebBundle:User:profile_edit.html.twig" %}

{% block title %}{{ parent() }} - Modificar Perfil{% endblock %}

{% block javascripts %}
	{{ parent() }}
	<script type="text/javascript">
		var selectedCity = {% if selectedcity %}{{ selectedcity.id }}{% else %}null{% endif %};
                                    var selectedTeam = {% if selectedteam %}{{ selectedteam.id }}{% else %}null{% endif %};
	</script>
	<script src="{{ asset('bundles/dodicifansworldweb/js/userinfo_common.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bundles/dodicifansworldweb/js/edit_profile.js') }}" type="text/javascript"></script>
{% endblock %}

{% block user_edit_toolbar %}
	{% set current_edit_toolbar = 'profile' %}
	{{ parent() }}
{% endblock %}

{% block profile_tabs %}
	{% set active_tab = 'detail' %}
	{{ parent() }}
{% endblock %}

{% block inner_content %}
	{% include "FOSUserBundle:Profile:edit_content.html.twig" %}
{% endblock %}