<section class='ranking-widget' >
    <header class="ranking-filters-container">
        {% include "DodiciFansworldWebBundle:Tv:blocks/filters.html.twig"  %}
    </header>
    
    <div class='content-container'>
        <div class='tag-list-container'>
            {% include "DodiciFansworldWebBundle:Tv:blocks/tag_list.html.twig"  %}
        </div>
        <div class='montage-container'>
            {% include "DodiciFansworldWebBundle:Video:blocks/montage.html.twig"  with { videos: videosDestacadosFW, listScrollable: true, listFilters:'#list-filters', listIsMontage: true } %}
        </div>
    </div>
</section>
<script>
    $(function(){
    	tv.init('.ranking-filters-container ul li','.channels-list',".ranking-widget [data-list-result]");
    });
</script>