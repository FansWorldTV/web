<ul class="video-list">
    {% for video in videos %}
        <li class="video-element">
            {% thumbnail video.image, 'huge_square' %}
            <span class="title">{{ video.title|truncate(24) }}</span>
            <span class="duration">{{ video.duration }}</span>
            <span class="overlay"></span>
            <span class="ico-play"></span>
        </li>
    {% endfor %}
</ul>