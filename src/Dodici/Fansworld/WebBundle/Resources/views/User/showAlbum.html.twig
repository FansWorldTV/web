{% extends "DodiciFansworldWebBundle:User:base.html.twig" %}

{% block title %}{{ parent() }} - {{ album.title }}{% endblock %}


{% block profile_tabs %}
	{% set active_tab = 'photos' %}
	{{ parent() }}
{% endblock %}


{% block profile_body %}
    {% embed "DodiciFansworldWebBundle:Layout/Embed:profile_inner_fullwidth.html.twig" %}
        {% block inner_content %}
        {% include 'DodiciFansworldWebBundle:Photo:blocks/album_montage.html.twig'  %}

        {% include 'DodiciFansworldWebBundle:Comment:blocks/box.html.twig' with {entity: album} %}

        {% endblock %}
    {% endembed %}
{% endblock %}
