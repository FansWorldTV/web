{% extends "DodiciFansworldWebBundle:Profile:base.html.twig" %}
{% if (user == app.user) %}
	{% set selfWall = true %}
{% else %}
	{% set selfWall = false %}
{% endif %}

{% set fship = appstate.friendshipwith(user) %}

{% if (appstate.canfriend(user) and fship is null) or ( isHome is defined and isHome) %}
	{% set tplLarge = true %}
{% else %}
	{% set tplLarge = false %}
{% endif %}

{% set profile_head_class 	= 'user_profile_head' %}
{% set profile_body_class 	= 'user_profile_body' %}
{% set entity_type 			= 'User' %}
{% set entity 				= user %}

{% block title %}{{ parent() }} - {{ user }}{% endblock %}
{% block metafbimage %}{{ appmedia.imageurl(user.image, 'small_square') }}{% endblock %}
{% block metafbtype %}{{ appfacebook.type('user') }}{% endblock %}