{% extends "DodiciFansworldWebBundle::popup.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    <!-- File-upload -->
    <link rel="stylesheet" href="{{ asset('bundles/dodicifansworldweb/js/file-uploader/fileuploader.css') }}" type="text/css" media="all">
{% endblock %}
    
{% block javascripts %}
	<script src="{{ asset('bundles/dodicifansworldweb/js/popup.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bundles/dodicifansworldweb/js/file-uploader/fileuploader.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bundles/dodicifansworldweb/js/popup_upload_photo.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bundles/bazingaexposetranslation/js/translation.js') }}" type="text/javascript"></script>
{% endblock %}

{% block content %} 
	<div class="greybox">
		<div class="container-up">
			<div id="file-uploader"></div>
	        <div id='progressbar' class="progress progress-success progress-striped">
	            <div class="bar" style="width:0%;"></div>
	        </div>
	    </div>
    </div>
{% endblock %}