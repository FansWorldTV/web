{% extends "DodiciFansworldWebBundle:Profile:base.html.twig" %}

{% block header %}
    {% set section_selected = 'teams' %}
    {{ parent() }}
{% endblock %}

{% set tship = appstate.teamshipwith(team) %}

{% if (tship is null) or ( isHome is defined and isHome) %}
	{% set tplLarge = true %}
{% else %}
	{% set tplLarge = false %}
{% endif %}

{% set profile_head_class 	= 'team_profile_head' %}
{% set profile_body_class 	= 'team_profile_body' %}
{% set entity_type 			= 'Team' %}
{% set entity 				= team %}

{% block title %}{{ parent() }} - {{ team }}{% endblock %}