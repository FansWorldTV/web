{% extends "DodiciFansworldWebBundle:Team:base.html.twig" %}

{% use "DodiciFansworldWebBundle:Layout/Snippet:profile_inner_rlsidebar.html.twig"  %}

{% block title %}{{ parent() }} - Datos{% endblock %}

{% block profile_tabs %}
	{% set active_tab = 'info' %}
	{{ parent() }}
{% endblock %}

{% block inner_left_sidebar %}
    {{ parent() }}
    {% include "DodiciFansworldWebBundle:Default:blocks/info_list.html.twig" with { entity:team, personalData: teamData,  list_title: 'Datos:' } %}
{% endblock %}

{% block inner_content %}
    {{ parent() }}
        {% if team.content %}
            {% include "DodiciFansworldWebBundle:Default:blocks/text_box.html.twig" with { box_title: 'Sobre mi:', box_content: team.content, box_class: 'entity-content' } %}
        {% endif %}
{% endblock %}

