{% extends "DodiciFansworldWebBundle:Team:base.html.twig" %}


{% block title %}{{ parent() }} - Videos{% endblock %}

{% block profile_tabs %}
	{% set active_tab = 'videos' %}
	{{ parent() }}
{% endblock %}

{% block profile_body %}
    {% embed "DodiciFansworldWebBundle:Layout/Embed:profile_inner_fullwidth.html.twig" %}
        {% block inner_content %}
            <div data-list="video" data-entity-type="team" data-entity-id="{{team.id}}">
                {% include 'DodiciFansworldWebBundle:Default:blocks/button_group.html.twig'  %}
        
                {% include 'DodiciFansworldWebBundle:Video:blocks/montage.html.twig' with {'entityId': team.id, 'entityType': 'team'} %}
            </div>
        {% endblock %}
    {% endembed %}		
{% endblock %}