{% extends "SonataAdminBundle::standard_layout.html.twig" %}

{% block stylesheets %}
	{{ parent() }}
	 <link rel="stylesheet" href="{{ asset('bundles/dodicifansworldadmin/css/jquery-ui-timepicker-addon.css') }}" type="text/css" media="all">
{% endblock %}

{% block javascripts %}
	{{ parent() }}
	<script src="{{ asset('bundles/dodicifansworldadmin/tinymce/tiny_mce.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/dodicifansworldadmin/js/jquery-ui-timepicker-addon.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/dodicifansworldadmin/js/localization/jquery-ui-timepicker-es.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/dodicifansworldadmin/js/common.js') }}" type="text/javascript"></script>
{% endblock %}

{% block footer %}
<div class="pull-right clearfix">
	<span class="label"><a href="http://www.dodici.com.ar" rel="noreferrer" style="text-decoration: none; color: black">Dodici Digital</a></span>
</div>
{% endblock %}

{% block logo %}
    <a href="{{ url('sonata_admin_dashboard') }}" class="brand">
        <img src="{{ asset(admin_pool.titlelogo) }}"  alt="{{ admin_pool.title }}" />
    </a>
{% endblock %}